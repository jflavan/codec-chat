		"typescript": "^5.9.3",
		"vite": "^7.3.1",
		"workbox-window": "^7.4.0"
	},
	"dependencies": {
		"@microsoft/signalr": "^10.0.0",
		"svelte-youtube-embed": "^0.4.6"
	},
